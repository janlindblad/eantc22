      tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='huawei-system-event-management']";
      description
        "System Event Management (SEM).";
      container slave-switch {
        description
          "Configure swithover.";
        leaf enable {
          type slave-switch-mode;
          default "enable";
          description
            "Indicates whether to enable slave switch.";
        }
        // NCS drop config false leaf status
