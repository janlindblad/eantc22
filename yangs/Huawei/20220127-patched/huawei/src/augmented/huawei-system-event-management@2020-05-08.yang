  augment "/ncs:devices/ncs:device/ncs:rpc" {
    container rpc-switch-over {
      tailf:ncs-device-type "netconf";
      tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='huawei-system-event-management']";
      tailf:action switch-over {
        tailf:actionpoint ncsproxy {
          tailf:internal;
        }
        description
          "Switch over.";
        input {
          leaf chassis-name {
            type string {
              length "1..31";
            }
            description
              "Chassis name.";
          }
          leaf position {
            type string {
              length "1..32";
            }
            description
              "Chassis position.";
          }
        }
      }
    }
  }
