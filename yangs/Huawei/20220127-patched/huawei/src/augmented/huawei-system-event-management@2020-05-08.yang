        // NCS drop config false leaf 'slave-board-postion'
      }
      container multi-chassis-slave-switch {
        description
          "Configure multi chassis swithover list.";
        list config {
          key "chassis-name";
          ext:generated-by "system";
          ext:operation-exclude "create";
          description
