    type enumeration {
      enum "enable" {
        value 0;
        description
          "Enable.";
      }
      enum "disable" {
        value 1;
        description
          "Disable.";
      }
    }
    description
      "Switchover enable type.";
  }

  typedef ncs_rpc_notif_slave-switch-policy {
    type enumeration {
      enum "board-switchover" {
        value 0;
