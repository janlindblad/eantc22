  grouping ncs_rpc_notif_bgp-scan-parameters {
    description
      "BGP grouping for BGP scan attributes";
    leaf scan-remaining-time {
      type uint8;
      units "seconds";
      description
        "Remaining scan timer";
    }
    list next-hop-cache {
      key "address";
      description
        "List of next-hops";
      leaf address {
        type inet:ip-address;
        description
