      description
        "List of next-hops";
      leaf address {
        type inet:ip-address;
        description
          "Address of the BGP peer either in IPv4 or IPv6";
      }
      leaf registered {
        type boolean;
        description
          "Next-hop is registered or not";
      }
    }
