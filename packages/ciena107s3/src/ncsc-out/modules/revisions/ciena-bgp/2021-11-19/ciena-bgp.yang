      leaf warning-only {
        type boolean;
        default "false";
        description
          "Using this option does not teardown the session, but
           instead throws a notification warning that the threshold
           has reached.";
      }
    }
    list prefix-list {
      key "name direction";
      max-elements 2;
      description
